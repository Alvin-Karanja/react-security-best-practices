#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dev/WebstormProjects/react-security-best-practices/node_modules/.pnpm/pacote@11.3.5/node_modules/pacote/lib/node_modules:/home/dev/WebstormProjects/react-security-best-practices/node_modules/.pnpm/pacote@11.3.5/node_modules/pacote/node_modules:/home/dev/WebstormProjects/react-security-best-practices/node_modules/.pnpm/pacote@11.3.5/node_modules:/home/dev/WebstormProjects/react-security-best-practices/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dev/WebstormProjects/react-security-best-practices/node_modules/.pnpm/pacote@11.3.5/node_modules/pacote/lib/node_modules:/home/dev/WebstormProjects/react-security-best-practices/node_modules/.pnpm/pacote@11.3.5/node_modules/pacote/node_modules:/home/dev/WebstormProjects/react-security-best-practices/node_modules/.pnpm/pacote@11.3.5/node_modules:/home/dev/WebstormProjects/react-security-best-practices/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../pacote@11.3.5/node_modules/pacote/lib/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../pacote@11.3.5/node_modules/pacote/lib/bin.js" "$@"
fi
